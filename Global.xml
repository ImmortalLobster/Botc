<TableLayout
    id = "whisperUI"
    height = "100"
    width = "250"
    cellSpacing = "0.5"
    rectAlignment = "LowerCenter"
    offsetXY = "125 7"
    visibility = "White|Brown|Red|Orange|Green|Teal|Blue|Purple|Pink"
    >
    <!-- Row 1 -->
    <Row preferredHeight = "30">
        <Cell
            dontUseTableCellBackground = "true"
            columnSpan = "1">
            <Dropdown
                id = "whisperDropdown"
                onValueChanged="whisperColorUI"
                colors = "#66666680|#666666FF|#66666680|#66666680"
                itemBackgroundColors = "#000000EE|#000000FF|#00000080|#00000080"
                scrollbarColors = "#000000EE|#000000FF|#00000080|#00000080"
                textColor = "#FF0000"
                checkColor = "#FF0000"
                arrowColor = "#FFFFFF"
                dropdownHeight = "500"
                >
            <Option selected="true">Select a Color</Option>
            </Dropdown>
        </Cell>
        <Cell
            dontUseTableCellBackground = "true"
            columnSpan = "1">
            <Button
                id = "sendMessageButton"
                onClick = "sendWhisperUI"
                colors = "#66666680|#666666FF|#66666680|#66666680"
                textColor = "#FFFFFF"
				text = "Send"
				fontSize = "15"
                >
            </Button>
        </Cell>
    </Row>
    <Row preferredHeight = "70">
        <Cell
            dontUseTableCellBackground = "true"
            columnSpan = "2">
            <InputField
                id = "whisperInput"
                onEndEdit = "whisperInputUI"
                placeholder = "Whisper to a player."
                colors = "#66666680|#666666FF|#66666680|#66666680"
                textColor = "#FFFFFF"
                lineType = "MultiLineNewLine"
                >
            </InputField>
        </Cell>
    </Row>
</TableLayout>

